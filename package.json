{
  "name": "@captive/gitlab-ci",
  "version": "1.0.0",
  "private": true,
  "description": "Shared Captive GitlabCI configuration",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run docs",
    "docs": ":",
    "format": "npx eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml --fix",
    "lint": "npx eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "pretest": "envhandlebars < 'test/test.gitlab-ci.yml.hbs' > 'test/test.gitlab-ci.yml'",
    "release": "scripts/release",
    "spellcheck": "npx cspell '**/*.md' --no-progress",
    "test": "exit 0;npx gitlab-ci-lint test/test.gitlab-ci.yml --url https://git.captive.fr --token=${GITLAB_LINT_TOKEN:-GITLAB_TOKEN}"
  },
  "eslintConfig": {
    "extends": [
      "@captive/eslint-config"
    ],
    "root": true
  },
  "eslintIgnore": [],
  "devDependencies": {
    "@captive/cspell-config": "2.1.1",
    "@captive/eslint-config": "2.0.4",
    "danger": "11.3.0",
    "envhandlebars": "1.4.8",
    "eslint": "8.52.0",
    "gitlab-ci-lint": "1.1.0",
    "prettier": "3.0.3"
  },
  "packageManager": "yarn@4.0.0",
  "engines": {
    "node": "20"
  }
}
