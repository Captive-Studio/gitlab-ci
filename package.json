{
  "name": "@captive/gitlab-ci",
  "version": "1.0.0",
  "private": true,
  "description": "Shared Captive GitlabCI configuration",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run docs",
    "docs": ":",
    "format": "npx eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml --fix",
    "lint": "npx eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "pretest": "envhandlebars < 'test/test.gitlab-ci.yml.hbs' > 'test/test.gitlab-ci.yml'",
    "release": "scripts/release",
    "test": "npx gitlab-ci-lint test/test.gitlab-ci.yml --url https://git.captive.fr --token=${GITLAB_TOKEN:-$CI_JOB_TOKEN}"
  },
  "eslintConfig": {
    "extends": [
      "@captive/eslint-config"
    ],
    "root": true
  },
  "eslintIgnore": [],
  "devDependencies": {
    "@captive/eslint-config": "1.7.18",
    "envhandlebars": "1.4.8",
    "eslint": "8.35.0",
    "gitlab-ci-lint": "1.1.0",
    "prettier": "2.8.4"
  },
  "packageManager": "yarn@3.4.1"
}
